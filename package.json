{
  "name": "@diegofigs/call-of-duty",
  "version": "0.1.0",
  "description": "Type safe wrapper for the private Call Of Duty API. Drop-in replacement to https://www.npmjs.com/package/call-of-duty-api.",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "type": "module",
  "source": "src/index.ts",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.modern.js"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "unpkg": "./dist/index.umd.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "microbundle --format=esm,cjs,modern --target=node",
    "dev": "microbundle watch",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diegofigs/call-of-duty.git"
  },
  "keywords": [
    "call-of-duty",
    "call of duty",
    "warzone",
    "modern warfare",
    "black ops",
    "battle royale",
    "cold war",
    "vanguard",
    "Treyarch",
    "Infinity Ward",
    "Sledgehammer",
    "Raven"
  ],
  "bugs": {
    "url": "https://github.com/diegofigs/call-of-duty/issues"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/diegofigs"
  },
  "author": "Diego Figueroa",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/node": "^17.0.45",
    "jest": "^28.1.3",
    "microbundle": "^0.15.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.3",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.3"
  },
  "dependencies": {
    "tslib": "^2.4.0",
    "undici": "^5.5.1"
  },
  "release": {
    "branches": [
      "main",
      "next"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
